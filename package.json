{
  "name": "create-react-node-app",
  "version": "0.0.0-development",
  "keywords": [
    "build-tools",
    "create-react-app",
    "node",
    "react",
    "zero-configuration"
  ],
  "homepage": "https://saiichihashimoto.github.io/create-react-node-app/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/create-react-node-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/create-react-node-app.git"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "bin": {
    "react-node-scripts": "./lib/react-node-scripts.js"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps --delete-dir-on-start --verbose --ignore \"**/*.spec.js\" && cp src/*.json lib && cp src/Procfile lib",
    "test": "jest --verbose $(if [ -z ${CI+x} ]; then echo --watch; else echo --color --coverage; fi)",
    "test-build": "cd test && npm ci && npm run build && npm run test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "babel.config.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "dependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.0",
    "@babel/node": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.0",
    "@babel/preset-env": "7.8.2",
    "@babel/preset-react": "7.8.0",
    "babel-plugin-css-modules-transform": "1.6.2",
    "babel-plugin-extension-resolver": "1.0.9",
    "babel-plugin-file-loader": "2.0.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-universal-dotenv": "1.2.1",
    "bunyan": "1.8.12",
    "commander": "4.0.1",
    "cross-env": "7.0.0",
    "execa": "4.0.0",
    "foreman": "3.0.1",
    "forever": "1.0.0",
    "listr": "0.14.3",
    "mongodb-prebuilt": "6.5.0",
    "ngrok": "3.2.7",
    "node-sass": "4.13.0",
    "nodemon": "2.0.2",
    "react-scripts": "3.0.1",
    "redis-prebuilt": "1.0.44",
    "universal-dotenv": "3.0.4",
    "update-notifier": "4.0.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.0.2",
    "eslint-config-saiichihashimoto": "2.1.0",
    "husky": "4.2.1",
    "jest": "24.1.0",
    "lint-my-app": "3.7.46",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "stylelint-config-saiichihashimoto": "2.0.16"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "engines": {
    "node": "12.8.0"
  }
}
